               class="com.ydpp.eclipse.actions.xml.AddAllNamespaceAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.xml.AddAllNamespaceAction"
               label="Add all namespace"
               menubarPath="menu2/namespace"
               tooltip="will declare spring all namespace"/>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.ydpp.eclipse.preferences.CodePreferencePage"
            id="com.ydpp.eclipse.preferences.CodePreferencePage"
            name="Code generate">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.ydpp.eclipse.preferences.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
