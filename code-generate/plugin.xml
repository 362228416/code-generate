               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.xml.AddTxNamespaceAction"
               label="Add tx namesapce"
               menubarPath="menu2/namespace"/>
         <action
               class="com.ydpp.eclipse.actions.xml.AddAopNamespaceAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.xml.AddAopNamespaceAction"
               label="Add aop namesapce"
               menubarPath="menu2/namespace"/>
         <action
               class="com.ydpp.eclipse.actions.xml.AddContextNamespaceAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.xml.AddContextNamespaceAction"
               label="Add context namesapce"
               menubarPath="menu2/namespace"/>
