<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            adaptable="true"
            id="code_generate.contribution1"
            
            objectClass="org.eclipse.core.runtime.IAdaptable">
         <menu
               label="Code Generate"
               id="menu1">
            <separator
                  name="generate">
            </separator>
         </menu>
         <action
               label="Genarate Repository"
               icon="icons/ico.gif"
               class="com.ydpp.eclipse.actions.classes.GenarateRepositoryAction"
               menubarPath="menu1/generate"
               id="code_generate.action1">
         </action>
         <action
               class="com.ydpp.eclipse.actions.classes.GenerateServiceAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.xml.GenerateServiceAction"
               label="Generate Service"
               menubarPath="menu1/generate"/>
         <action
               class="com.ydpp.eclipse.actions.classes.GenerateControllerAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.classes.GenerateControllerAction"
               label="Genarate Controller"
               menubarPath="menu1/generate"/>
		<action
               class="com.ydpp.eclipse.actions.classes.GenerateRepositoryAndServiceAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.classes.GenerateRepositoryAndServiceAction"
               label="Genarate Repository And Service"
               menubarPath="menu1/generate"/>               
		<action
               class="com.ydpp.eclipse.actions.classes.GenerateRepositoryAndControllerAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.classes.GenerateRepositoryAndControllerAction"
               label="Genarate Repository And Controller"
               menubarPath="menu1/generate"/>               
		<action
               class="com.ydpp.eclipse.actions.classes.GenerateRepositoryAndServiceAndControllerAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.classes.GenerateRepositoryAndServiceAndControllerAction"
               label="Genarate Repository And Service And Controller"
               menubarPath="menu1/generate"/>               
		<action
               class="com.ydpp.eclipse.actions.method.GenerateToJsonMethodAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.method.GenerateToJsonMethodAction"
               label="Genarate ToJson Method"
               menubarPath="menu1/generate"/>               
         <action
               class="com.ydpp.eclipse.actions.classes.GeneratePageAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.classes.GeneratePageAction"
               label="Genarate Page"
               menubarPath="menu1/generate"/>
         <menu
               id="menu2"
               label="Spring Bean Configuration"
               >
            <separator name="namespace"/>
            <separator name="bean"/>
            <separator name="mvc"/>
         </menu>
         <action
               class="com.ydpp.eclipse.actions.xml.AddAnnotationConfigAction"
               icon="icons/ico.gif"
               id="com.ydpp.eclipse.actions.xml.AddAnnotationConfigAction"
               label="Add Annotation Configuration"
               menubarPath="menu2/bean"/>
         <action
